name: preprocessing_step

# The conda environment 
conda_env: conda.yaml

# The entry points define how to run this stage.
entry_points:
    main:
        parameters:
            input_data:
                description: The file path to raw csv dataset
                type: path
            output_data:
                description: The name of the output data file with extension .csv
                type: string 
        command: python preprocessing.py --input_data={input_data}  --output_data={output_data}
        
        